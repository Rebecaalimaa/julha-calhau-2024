#include<stdio.h>

 int main()
 
{

     float velocidade = 900;
     float distancia; 
     float tempo;

      printf("Digite a distância a ser percorrida em km:\n");
      scanf("%f", &distancia);
      
      tempo = distancia / velocidade;
       
      printf("A sua velocidade é: %f ; " , distancia / velocidade);
      
      
      return 0;
      
}
